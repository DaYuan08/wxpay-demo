<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuan.java.wxpay.demo.mapper.BookMapper">

    <select id="selectBook" resultType="com.yuan.java.wxpay.demo.domain.Book">
        SELECT `id`, `title`, `author`, `rating`, `price`, `cover`, `create_time`, `update_time` FROM `book` ORDER BY `rating` DESC
    </select>

</mapper>